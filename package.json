{
  "name": "ember-form-builder",
  "version": "2.0.0",
  "description": "Simple and easily extensible form builder for Ember apps",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli": "~3.18.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-decorators": "^6.1.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-sinon": "^4.0.0",
    "ember-source": "~3.18.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.6.0",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-ember": "^8.4.0",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.2.0"
  },
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nibynic/ember-form-builder"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "dependencies": {
    "@ember/render-modifiers": "^1.0.2",
    "babel-plugin-ember-modules-api-polyfill": "^2.3.2",
    "ember-classic-decorator": "^1.0.7",
    "ember-cli-babel": "^7.19.0",
    "ember-cli-htmlbars": "^4.3.1",
    "ember-inflector": "^3.0.0",
    "ember-test-selectors": "^3.0.0",
    "ember-truth-helpers": "^2.1.0"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "versionCompatibility": {
      "ember": ">3.16.4"
    }
  },
  "resolutions": {
    "clean-css": "^4.1.11",
    "minimist": "^0.2.1"
  }
}
