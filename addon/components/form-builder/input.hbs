{{#let (component (concat "input-wrappers/" this.wrapper)) as |Wrapper|}}
  <Wrapper
    @config={{this.config}}
    @inputComponent={{component (concat "inputs/" this.type "-input") config=this.config}}
    @labelComponent={{component "form-builder/label" for=this.inputElementId isRequired=this.validations.required text=this.texts.label}}
    {{on "focusout" this.handleFocusOut}}
    data-test-input-type={{this.type}}
    data-test-input-name={{this.name}}
    ...attributes
  />
{{/let}}
